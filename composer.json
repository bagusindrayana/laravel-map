{
    "name": "bagusindrayana/laravel-map",
    "description": "map view for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagus Indrayana",
            "email": "bagusindrayanaindo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "nikic/php-parser": "4.10"
    },
    "autoload": {
